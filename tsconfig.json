
{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": false,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "declaration": true,
    "declarationDir": "dist/@types",
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es5",
      "es2015",
      "es6",
      "dom",
      "es2017.object"
    ],
    "sourceMap": true,
    "outDir": "./dist/",
    "baseUrl": "."
  }
}